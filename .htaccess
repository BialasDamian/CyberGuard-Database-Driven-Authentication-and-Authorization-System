# *** Opcje Wyszukiwarki
Options -Indexes

# *** WstÄ™pna Konfiguracja
RewriteEngine On
RewriteBase /

# *** Przyjazny Adres URL
RewriteCond %{HTTP_HOST} !^db.crm.pl [NC]
RewriteRule ^(.*)$ http://db.crm.pl%{REQUEST_URI} [R=301,L]
RewriteRule ^/?([a-zA-Z0-9-]*)$ /index.php?WWW=$1 [NC,L]
RewriteRule ^/?([a-zA-Z0-9-]*),([a-zA-Z0-9-]*)$ /index.php?WWW=$1&WW=$2 [NC,L]
RewriteRule ^/?([a-zA-Z0-9-]*),([a-zA-Z0-9-]*),([a-zA-Z0-9-]*)$ /index.php?WWW=$1&WW=$2&W=$3 [NC,L]

ErrorDocument 404 http://db.crm.pl//404
